  },
  "lint-staged": {
    "**/*.(js|jsx)": [
      "eslint --ext .js,.jsx --no-eslintrc -c .eslintrc.js.json --fix"
    ],
    "**/*.(ts|tsx)": [
      "eslint --ext .ts,.tsx --no-eslintrc -c .eslintrc.json --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
