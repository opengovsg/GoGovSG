{
  "name": "GoGovSG",
  "version": "1.19.0",
  "description": "Link shortener for Singapore government.",
  "main": "src/server/index.js",
  "scripts": {
    "lint-js": "eslint --ext .js,.jsx --no-eslintrc -c .eslintrc.json --ignore-path .gitignore .",
    "lint-ts": "eslint --ext .ts --no-eslintrc -c .eslintrc.ts.json --ignore-path .gitignore .",
    "lint-js:fix": "eslint --ext .js,.jsx --no-eslintrc -c .eslintrc.json --ignore-path .gitignore --fix .",
    "lint-ts:fix": "eslint --ext .ts --no-eslintrc -c .eslintrc.ts.json --ignore-path .gitignore --fix .",
    "lint": "npm run lint-js && npm run lint-ts",
    "lint:fix": "npm run lint-js:fix && npm run lint-ts:fix",
    "build": "tsc && webpack --mode production",
    "postbuild": "copyfiles -u 1 src/server/views/**/* build && copyfiles -u 3 src/server/util/assets/**/* build/server/util",
    "start": "node build/server/index.js",
    "client-dev": "webpack-dev-server --mode development --host 0.0.0.0 --devtool inline-source-map --hot",
    "server-dev": "ts-node-dev --respawn --transpileOnly --inspect=0.0.0.0 -- src/server/index.ts",
    "docker-dev": "concurrently \"npm run server-dev\" \"npm run client-dev\"",
    "dev": "docker-compose -f docker-compose.yml up --build",
    "test": "jest --collectCoverage",
    "test:ci": "jest --coverage && coveralls < coverage/lcov.info",
    "cz": "git-cz"
  },
  "author": "Yuanruo Liang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opengovsg/GoGovSG.git"
  },
  "dependencies": {
