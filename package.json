    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@sentry/webpack-plugin": "^1.10.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.26",
    "@types/connect-redis": "0.0.10",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.12",
    "@types/helmet": "0.0.43",
    "@types/morgan": "^1.7.35",
