    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.0.7",
    "nodemon": "^1.18.11",
    "style-loader": "^0.23.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.7.2",
    "url-loader": "^1.0.1",
