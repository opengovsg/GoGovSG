    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && commitlint --from origin/develop --to HEAD --verbose",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "sh secrets-check.sh && lint-staged"
    }
  },
  "lint-staged": {
    "**/*.(js|jsx)": [
      "eslint --ext .js,.jsx --no-eslintrc -c .eslintrc.json --fix"
    ],
    "**/*.ts": [
      "eslint --ext .ts --no-eslintrc -c .eslintrc.ts.json --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
